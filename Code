n=int(input())
nl=n
nh=n
while True:
    rev=int(str(nl)[::-1])
    if nl==rev:
        low=nl
        print(low)
        break
    else:
        nl=nl-1
print(nh)
while True:
    rev=int(str(nh)[::-1])
    if nh==rev:
        high=nh
        print(high)
        break
    else:
        nh=nh+1
if n-low<high-n:
    print(low)
else:
    print(high)
